#include<iostream>
using namespace std;
int s[20][20],m[20][20],p[20];
void MCO(int n)
{ int l,i,j,k,q;
  for(i=1;i<=n;i++)
  { m[i][i]=0;}
  for(l=2;l<=n;l++)
  {for(i=1;i<=n-l+1;i++)
   { j=i+l-1;
     m[i][j]=100000000000;
     for(k=i;k<=j-1;k++)
     { q=m[i][k]+m[k+1][j]+p[i-1]*p[k]*p[j];
       if(q<m[i][j])
       { m[i][j]=q;
         s[i][j]=k; }}}}}
void printsol(int i,int j)
{ if(i==j)
  { cout<<"A["<<i<<"]";}
  else
  { cout<<"(";
    printsol(i,s[i][j]);
    printsol(s[i][j]+1,j);
    cout<<")";}}
int main()
{ int n,i;
  cout<<"How many matrices: ";
  cin>>n;
  cout<<"\nEnter the dimensions of matrix row1 col1 col2 ....col n: ";
  for(i=0;i<=n;i++)
  { cin>>p[i];}
  MCO(n);
  cout<<"\nSolution: ";
  printsol(1,n);
  return 0;}
